<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.ekaterinakubrina.wordsen">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WordsEN">
        <activity
            android:name=".DictionaryActivity"
            android:label="@string/title_activity_dictionary"
            android:theme="@style/Theme.WordsEN.NoActionBar" />
        <activity android:name=".MainEntryActivity" />
        <activity
            android:name=".TestActivity"
            android:allowBackup="false"
            android:label="@string/title_activity_test"
            android:theme="@style/Theme.WordsEN.NoActionBar" />
        <activity android:name=".SelectLevelActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EntryActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".SuccessRegistrationActivity" />
        <activity android:name=".FirstInActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>