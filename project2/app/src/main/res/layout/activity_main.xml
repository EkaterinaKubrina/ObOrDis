<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="213dp"
        android:layout_height="106dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:textColor="#D3085F"
        app:drawableTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.919"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.521"
        tools:text="@string/String" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="116dp"
        android:layout_height="71dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textColor="#D3085F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.045"
        tools:text="@string/String" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="156dp"
        android:layout_height="90dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textColor="#D3085F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.89"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067"
        tools:text="@string/String" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="373dp"
        android:layout_height="100dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#D3085F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.683"
        tools:text="@string/String" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="141dp"
        android:layout_height="72dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#D3085F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.388"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.165"
        tools:text="@string/String" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="131dp"
        android:layout_height="75dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#D3085F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.803"
        tools:text="@string/String" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#D3085F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_counter"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.407"
        tools:text="@string/String" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="215dp"
        android:layout_height="90dp"
        android:layout_marginBottom="40dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:textColor="#D3085F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.234"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_counter"
        app:layout_constraintVertical_bias="0.972"
        tools:text="@string/String" />

    <Button
        android:id="@+id/button"
        android:layout_width="188dp"
        android:layout_height="67dp"
        android:layout_marginTop="178dp"
        android:text="@string/Поздороваться"
        app:backgroundTint="#CA2A60"
        app:iconTint="#FAF2F2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Поздороваться" />

    <Button
        android:id="@+id/button_counter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="255dp"
        android:layout_marginEnd="114dp"
        android:layout_marginRight="114dp"
        android:layout_marginBottom="245dp"
        android:text="@string/Считаем_клики"
        app:backgroundTint="#DD94B4"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Считаем клики" />

</androidx.constraintlayout.widget.ConstraintLayout>